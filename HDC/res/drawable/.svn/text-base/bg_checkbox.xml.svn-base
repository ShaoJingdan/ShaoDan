<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/icon_checked_focus" android:state_checked="true" android:state_focused="true"/>
 <!-- 选中有焦点时效果 -->
    <item android:drawable="@drawable/icon_checked_normal" android:state_checked="true" android:state_focused="false"/>
 <!-- 选中无焦点时效果 -->
    <item android:drawable="@drawable/icon_unchecked_focus" android:state_checked="false" android:state_focused="true"/>
 <!-- 未选中有焦点时效果 -->
    <item android:drawable="@drawable/icon_unchecked_normal" android:state_checked="false" android:state_focused="false"/>
 <!-- 未选中无焦点时效果 -->

</selector>